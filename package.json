{
  "name": "k8r",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'nodemon server/server.ts'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' server/server.ts",
    "dev2": "concurrently 'vite' 'nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' server/server.ts'",
    "dev:client": "'vite'",
    "devAI": "concurrently 'vite' 'nodemon --watch './src/**/*.ts' --exec 'ts-node' server/server.ts'"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.8.4",
    "passport-google-oidc": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.6.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "cli-color": "^2.0.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
